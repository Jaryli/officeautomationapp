<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="shape">
            <enum name="circle" value="0" />
            <enum name="fillet" value="1" />
        </attr>
        <attr name="radius" format="dimension" />
        <attr name="cover_color" format="color" />
    </declare-styleable>

    <declare-styleable name="MyImageView">
        <attr name="auto_play" format="boolean">
        </attr>
    </declare-styleable>

    <declare-styleable name="MoreTextStyle">
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="maxLine" format="integer" />
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="app">
        <attr name="bottomDragVisibleHeight" format="dimension" />
        <attr name="bototmExtraIndicatorHeight" format="dimension" />
    </declare-styleable>
</resources>